  <div class="jumbotron">
    <div class="container">
      <h1 class="title">Scrape AppleInsider</h1>
      <p class="lead">Click the button to scrape articles from AppleInsider.com</p>
      <img src="../img/icon.svg" width=45px>&nbsp;
      <button type="button" class="btn btn-primary btn-lg">Scrape AppleInsider</button>
    </div>
  </div>

<div class="container" id="panelContainer">
    {{#each article}}
    <div class="panel panel-default" id={{this._id}}>
       <div class="panel-heading">
            <a href={{this.link}} target="_blank">
               <h4>{{this.title}}</h4>
            </a>
        </div>
        <div class="panel-body">
            <p>{{this.postDescription}}</p>
            <button class='btn btn-primary save-article'  id='saveArticleButton'>Save Article</button>
        </div>
        
    </div>
    {{/each}}
</div>